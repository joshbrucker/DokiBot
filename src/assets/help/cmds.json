{
    "end": {
        "footer": {
            "text": "*Needs \"Manage Server\" Permission"
        },
        "description": "Ends the current poem in the doki-poems channel.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pend`\n\u200b"
            }
        ]
    },
    "frequency": {
        "footer": {
            "text": "*Needs \"Manage Server\" Permission"
        },
        "description": "Changes the frequency DokiBot grabs words.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pfrequency <interval>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pfrequency minute`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`<interval>` minute, hour, day\n\u200b"
            }
        ]
    },
    "submit": {
    	"description": "Submits an insult to be accepted or rejected by DokiBot.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%psubmit <insult>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pinsult %user% just submitted an insult!`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`<insult>` Any string of text. Each insult **must** contain at least one `%user%`, which DokiBot will later replace with a random user.\n\u200b"
            }
        ]
    },
    "toggle": {
    	"footer": {
            "text": "*Needs \"Manage Server\" Permission"
        },
        "description": "Turns on/off the random insult functionality.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%ptoggle`\n\u200b"
            }
        ]
    },
    "anime": {
    	"description": "Finds info on a given anime.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%panime <title>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%panime steins;gate`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`<title>` Any valid anime title"
            }
        ]
    },
    "moniquote": {
    	"description": "Posts one of Monika's quotes from the end of the DDLC.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pmoniquote`"
            }
        ]
    },
    "doki": {
    	"description": "Posts an image of a Doki girl.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pdoki [tags]`"
            },
                        {
                "name": "**__Example__**",
                "value": "`%pdoki sayori`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`[tags]` *nothing*, nsfw, 1girl, 2girls, multiple, monika, sayori, yuri, natsuki"
            }
        ]
    },
    "neko": {
    	"description": "Posts an image of a beautiful neko.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pneko [tags]`"
            },
            {
            	"name": "**__Example__**",
            	"value": "`%pneko gif"
            },
            {
                "name": "**__Arguments__**",
                "value": "`[tags]` *nothing*, nsfw, gif"
            }
        ]
    },
    "waifu": {
    	"description": "Posts an image of a waifu.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pwaifu [tags]`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pwaifu texas arknights$feet`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`[tags]` *nothing*, nsfw, questionable, explicit, gif, sound, any valid Danbooru tag\n\u200b"
            },
            {
                "name": "**__Additional Info__**",
                "value": "\u2022 Tags are split with the $ symbol.\n\n\u2022 Only allows up to 4 tags.\n\n\u2606\u2606\u2606 No need for underscores anymore! Our new algorithm will automatically try to find a tag using whatever you give it. Sometimes you still have to give it the series name, though."
            }
        ]
    },
    "nep": {
    	"description": "Plays a soundclip from Hypderdimension Neptunia.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pnep [character]`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pnep neptune`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`[character]` *nothing*, neptune, vert, noire, blanc, nepgear, uni, rom, ram, if, compa"
            }
        ]
    },
    "play": {
    	"description": "Plays a track from the DDLC OST.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pplay <number>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pplay 13`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`<number>` numbers 1 to 15"
            }
        ]
    },
    "playall": {
    	"description": "Queues all tracks from the DDLC OST.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pplayall`"
            }
        ]
    },
    "list": {
    	"description": "Lists all songs from the DDLC OST along with their numbers.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%plist`"
            }
        ]
    },
    "stop": {
    	"description": "Stops the current song and clears the queue.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pstop`"
            }
        ]
    },
    "pause": {
    	"description": "Pauses the current song.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%ppause`"
            }
        ]
    },
    "resume": {
    	"description": "Resumes a paused song.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%presume`"
            }
        ]
    },
    "skip": {
    	"description": "Skips the current song.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pskip`"
            }
        ]
    },
    "queue": {
    	"description": "Displays the song queue.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pqueue`"
            }
        ]
    },
    "clear": {
    	"description": "Wipes the entire queue.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pclear`"
            }
        ]
    },
    "help": {
    	"description": "Isn't this meta? Lists all commands or info about a specific command.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%phelp [command]`"
            },
            {
                "name": "**__Example__**",
                "value": "`%phelp waifu`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`[command]` *nothing*, any valid DokiBot command"
            }
        ]
    },
    "setchannel": {
        "footer": {
            "text": "*Needs \"Manage Server\" Permission"
        },
        "description": "Changes the channel DokiBot will post insults and other messages to.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%psetchannel <channel>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%psetchannel #general`"
            },
            {
                "name": "**__Arguments__**",
                "value": "`<channel>` Any valid text channel in the Discord server\n\u200b"
            }
        ]
    },
    "prefix": {
    	"footer": {
            "text": "*Needs \"Manage Server\" Permission"
        },
        "description": "Changes the prefix used for DokiBot commands.\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pprefix <prefix>`"
            },
            {
                "name": "**__Example__**",
                "value": "`%pprefix -`"
            },
            {
                "name": "**__Arguments__**",
                "value": "**<prefix>** A prefix of your choice, at most 3 characters in length"
            }
        ]
    },
    "vote": {
    	"description": "Posts the voting link for DokiBot. Voting resets cooldowns!\n\u200b",
        "fields": [
            {
                "name": "**__Usage__**",
                "value": "`%pvote`"
            }
        ]
    }
}
